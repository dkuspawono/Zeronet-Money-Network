{
  "db_name": "MoneyNetwork",
  "db_file": "data/moneynetwork.db",
  "version": 2,
  "maps": {
    "users/.+/data.json": {
      "to_table": [ "search" ],
      "to_keyvalue": [ "sha256", "pubkey" ]
    }
  },
  "tables": {
    "search": {
      "cols": [
        ["tag", "TEXT"],
        ["value", "TEXT"],
        ["time", "INTEGER"],
        ["json_id", "INTEGER REFERENCES json (json_id)"]
      ],
      "indexes": ["CREATE INDEX search_i1 ON search(value, tag)"],
      "schema_changed": 1
    }
  }
}